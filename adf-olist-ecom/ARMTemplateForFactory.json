{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-olist-ecom"
		},
		"AdlsForCsv_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AdlsForCsv'"
		},
		"MySqlLink_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'MySqlLink'"
		},
		"AdlsForCsv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saolistdata.dfs.core.windows.net/"
		},
		"MySqlLink_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "luv43.h.filess.io"
		},
		"MySqlLink_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "olistProject_havinghang"
		},
		"MySqlLink_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "olistProject_havinghang"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AdlsForCsv')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AdlsForCsv_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('AdlsForCsv_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/MySqlLink')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "MySql",
				"typeProperties": {
					"server": "[parameters('MySqlLink_properties_typeProperties_server')]",
					"port": "3307",
					"database": "[parameters('MySqlLink_properties_typeProperties_database')]",
					"username": "[parameters('MySqlLink_properties_typeProperties_username')]",
					"sslMode": 1,
					"useSystemTrustStore": 0,
					"password": {
						"type": "SecureString",
						"value": "[parameters('MySqlLink_password')]"
					},
					"driverVersion": "v2"
				}
			},
			"dependsOn": []
		}
	]
}