{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-olist-ecom"
		},
		"AdlsForCsv_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AdlsForCsv'"
		},
		"MySqlLink_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'MySqlLink'"
		},
		"AdlsForCsv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saolistdata.dfs.core.windows.net/"
		},
		"MySqlLink_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "luv43.h.filess.io"
		},
		"MySqlLink_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "olistProject_havinghang"
		},
		"MySqlLink_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "olistProject_havinghang"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-olist-ecom_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdlsForCsv_accountKey": {
						"value": "[parameters('AdlsForCsv_accountKey')]"
					},
					"MySqlLink_password": {
						"value": "[parameters('MySqlLink_password')]"
					},
					"AdlsForCsv_properties_typeProperties_url": {
						"value": "[parameters('AdlsForCsv_properties_typeProperties_url')]"
					},
					"MySqlLink_properties_typeProperties_server": {
						"value": "[parameters('MySqlLink_properties_typeProperties_server')]"
					},
					"MySqlLink_properties_typeProperties_database": {
						"value": "[parameters('MySqlLink_properties_typeProperties_database')]"
					},
					"MySqlLink_properties_typeProperties_username": {
						"value": "[parameters('MySqlLink_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}